{
    "name": "free-geoip-app",
    "version": "1.0.0",
    "description": "A little application getting the ip address, geolocation data from a visitor who lands on the site",
    "main": "index.js",
    "scripts": {
        "scss": "node-sass --watch styles/scss -o styles/css",
        "clean": "rm -rf dist/ && mkdir dist",
        "build": "cp -R scripts styles dist/ && cp index.html dist/",
        "deploy": "gh-pages -d dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/interglobalmedia/geo-ip-app.git"
    },
    "author": "Maria D. Campbell",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/interglobalmedia/geo-ip-app/issues"
    },
    "homepage": "https://github.com/interglobalmedia/geo-ip-app#readme",
    "dependencies": {
        "gh-pages": "^2.2.0",
        "node-sass": "^4.13.1"
    }
}